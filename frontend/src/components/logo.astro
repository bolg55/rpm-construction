---
import { Image } from 'astro:assets';
const { logo } = Astro.props;
const { url, alternativeText: alt } = logo.image;
---

<a href={logo.href}>
  <Image
    src={`http://localhost:1337${url}`}
    alt={alt}
    width={100}
    height={150}
    quality={100}
    format='webp'
    loading='eager'
    decoding='sync'
    class='rounded-md stroke-white stroke-2'
  />
</a>
